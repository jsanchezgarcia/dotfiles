{
    "security.promptForLocalFileProtocolHandling": false,
    "workbench.tree.indent": 14,
    "workbench.tree.renderIndentGuides": "always",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.fontFamily": "Dank Mono, monospace",
    "editor.cursorSmoothCaretAnimation": "on",
    "explorer.confirmDelete": false,
    "terminal.integrated.fontFamily": "Dank Mono, monospace",
    "terminal.integrated.fontSize": 18,
    "git.enableSmartCommit": true,
    "githubPullRequests.pullBranch": "never",
    "Key Promoter.ignoredCommands": [
        "type",
        "workbench.action.closeActiveEditor"
    ],
    "Key Promoter.suggestKeybindingCreation": false,
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "cursor.composer.shouldChimeAfterChatFinishes": true,
    "cursor.composer.textSizeScale": 1.15,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "update.releaseTrack": "prerelease",
    "cursor.enable_agent_window_setting": true,
    "vsnotes.defaultNotePath": "~/Documents/vsnotes",
    "git.closeDiffOnOperation": true,
    "workbench.colorTheme": "Tokyo Night",
    "workbench.statusBar.visible": false,
    "editor.renderWhitespace": "none",
    "workbench.layoutControl.enabled": false,
    "findJump.dimWhenActive": true
}